{
  "compilerOptions": {
    "module": "amd",
    "target": "es5",
    "noImplicitAny": false,
    "removeComments": true,
    "sourceMap": false,
    "moduleResolution": "node",
    "baseUrl": "./src",
    "paths": {
      "@common/*": ["common/*"],
      "@toymvc/*": ["common/toymvc/*"]
    }
  },
  "exclude": [
    "node_modules",
    "build",
    "release"
  ]
}
